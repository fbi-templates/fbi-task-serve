    "url": "git+https://github.com/fbi-templates/fbi-task-serve.git"
  },
  "keywords": ["server", "static-server", "koajs", "fbi-task", "fbi"],
  "author": "neikvon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fbi-templates/fbi-task-serve/issues"
  },
  "homepage": "https://github.com/fbi-templates/fbi-task-serve#readme",
  "fbi": {
    "template": {
      "name": "fbi-task-serve"
    },
    "tasks": {
      "serve": {
        "alias": "s"
      }
    }
  }
}
